<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.app.inv.mapper.UnitMapper">
	<select id="loadUnit" resultType="com.app.inv.model.Unit">
		SELECT  
		code_unit as codeUnit, 
		name 
		FROM 
		core_units	
	</select>
</mapper>

